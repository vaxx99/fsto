{
  "name": "fsto",
  "version": "0.1.0",
  "main": "./main.js",
  "scripts": {
    "start": "electron .",
    "package":"electron-packager ~/.go/src/fsto fsto --platform=linux --arch=x64 --ignore=\"node_modules/(electron-packager|electron-prebuilt)\" --version=0.35.1 --overwrite",
    "build": "go build -o fsts fsts.go",
    "install": "rm -rf ~/.elc/apps/fsto | mv fsto-* ~/.elc/apps/fsto"
},
"files": [
"/fsts",
"/src/refresh.js",
"/css/style.css"
]
}
