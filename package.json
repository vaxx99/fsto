{
  "name": "fsto",
  "version": "0.1.0",
  "main": "./main.js",
  "scripts": {
    "start": "electron .",
    "pack32":"electron-packager ~/.go/src/fsto fsto --platform=linux --arch=ia32 --ignore=\"node_modules/(electron-packager|electron-prebuilt)\" --version=0.36.0 --overwrite",
    "pack64":"electron-packager ~/.go/src/fsto fsto --platform=linux --arch=x64 --ignore=\"node_modules/(electron-packager|electron-prebuilt)\" --version=0.36.0 --overwrite",
    "build": "go build -o fsts main.go",
    "install": "rm -rf ~/.elc/apps/fsto | mv fsto-* ~/.elc/apps/fsto"
}
,
"files": [
"/fsts",
"/css/style.css"
]
}

