{
  "name": "fsto",
  "version": "0.1.0",
  "main": "./main.js",
  "scripts": {
    "start": "electron .",
    "package":"electron-packager ~/.go/src/fsto fsto --platform=linux --arch=ia32 --icon=./fsto.ico --ignore=\"node_modules/(electron-packager|electron-prebuilt)\" --version=0.35.1 --overwrite",
    "build": "go build -o fsts fsts.go",
    "install": "rm -rf ~/.elc/apps/fsto | mv fstm-* ~/.elc/apps/fsto"
},
    "file": "./fsts"
}
